  var gr = new GlideRecord('sc_task');
  gr.addQuery('request_item',inputs.parent);
  gr.query();
  outputs.allClosed = 'Yes';
  gs.info('Log is: '+gr.getRowCount());
  while(gr.next()){
   if(gr.state != '3' &amp;&amp; gr.state!='4' &amp;&amp; gr.state != '7') {
     outputs.allClosed = 'No';
   }
  }
})(inputs, outputs);
